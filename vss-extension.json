{
  "manifestVersion": 1,
  "id": "apiswag",
  "name": "APIM OpenAPI Import",
  "version": "1.2.5",
  "publisher": "MegaTherionLabs",
  "tags": [
    "API",
    "OpenApi",
    "Swagger",
    "Security"
  ],
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "public": false,
  "links": {
    "getstarted": {
      "uri": "https://github.com/AlexanderViken/azure-apim-vsts"
    },
    "support": {
      "uri": "https://github.com/AlexanderViken/azure-apim-vsts/issues"
    }
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Release tasks for adding API to Azure API Management Service",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "apim"
    },
    {
      "path": "apimversioned"
    },
    {
      "path": "apimcreateorupdateproduct"
    },
    {
      "path": "apimfunction"
    },
    {
      "path": "apimfunctionversioned"
    },
    {
      "path": "apimfunctionv2versioned"
    },
    {
      "path": "apisecuritychecker"
    },
    {
      "path": "apimvalidator"
    },
    {
      "path": "images",
      "addressable": true
    }
  ],

  "contributions": [
    {
      "id": "apim",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apim"
      }
    },
    {
      "id": "apimval",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimvalidator"
      }
    },
    {
      "id": "apimversioned",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimversioned"
      }
    },
    {
      "id": "apisecuritychecker",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apisecuritychecker"
      }
    },
    {
      "id": "apimcreateorupdateproduct",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimcreateorupdateproduct"
      }
    },
    {
      "id": "apimfunction",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimfunction"
      }
    },
    {
      "id": "apimfunctionversioned",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimfunctionversioned"
      }
    },
    {
      "id": "apimfunctionv2versioned",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "apimfunctionv2versioned"
      }
    }
  ]
}